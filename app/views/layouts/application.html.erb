<!DOCTYPE html>
<html>
<head>
  <title>Uoi</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>


  <div class="navbar-fixed blue-grey ligthen-3">
    <nav class="blue-grey ligthen-3">
      <div class="nav-wrapper blue-grey ligthen-3">
        <a href="#!" class="brand-logo center blue-grey ligthen-3">UOI</a>
        <% if current_user %>
          <span>
            <%= current_user.username %>
          </span>

          <ul class="right">
            <li>
              <%= link_to "Log out", destroy_user_session_path, method: :delete %>
            </li>
          </ul>
        <% else %>
          <ul class="right">
            <li>
              <%= link_to "Log in", new_user_session_path %>
              <%= link_to "Sign up", new_user_registration_path %>
            </li>
          </ul>
        <% end %>
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
      </div>
    </nav>
  </div>

  <div class="container" id="jumbotron">

    <%= yield %>

  </div>


</body>
</html>
